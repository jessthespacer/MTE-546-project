clear all; close all;

%%
syms L Lr b c1 c2 real;
syms delta(t) T(t) v(t) theta(t) omega(t);

aF = c1*v + c2;
aD = b*v;

%%
vdot = aF - aD;
thetadot = tan(delta)*v/sqrt(L^2 + Lr^2*tan(delta)^2);

X = [v; theta; omega];

Xdot = [vdot; ...
        thetadot; ...
        simplify(diff(thetadot, t))];

%%
